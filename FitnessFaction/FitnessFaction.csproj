    <RootNamespace>FITNESS_FACTION_.NET_CORE_CONVERSIONS</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNet.Identity.Core" Version="2.2.3" />
